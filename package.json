{
  "name": "@simonorzel26/ai-models",
  "version": "2.0.3",
  "description": "AI model types extractor for @ai-sdk providers",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "node": {
        "import": {
          "types": "./dist/index.d.ts",
          "default": "./dist/index.js"
        },
        "require": {
          "types": "./dist/index.d.cts",
          "default": "./dist/index.cjs"
        }
      },
      "import": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "require": {
        "types": "./dist/index.d.cts",
        "default": "./dist/index.cjs"
      },
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "extract": "bun src/extract-model-types.ts",
    "build": "npm run extract && npm run bundle",
    "bundle": "tsup",
    "dev": "tsup --watch",
    "clean": "rm -rf dist",
    "typecheck": "tsc --noEmit",
    "test": "vitest run",
    "test:unit": "vitest run tests/unit tests/types",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:types": "vitest run --typecheck",
    "test:integration": "npm run build && vitest run tests/integration",
    "test:integration-only": "vitest run tests/integration",
    "test:all": "npm run typecheck && npm run test:unit && npm run bundle && npm run test:integration-only",
    "prepublishOnly": "npm run test:all",
    "release": "npm version patch && npm publish"
  },
  "keywords": [
    "ai",
    "models",
    "typescript",
    "ai-sdk",
    "openai",
    "anthropic",
    "mistral",
    "llm",
    "language-models",
    "model-types"
  ],
  "author": "Simon Orzel <simonorzel26@gmail.com>",
  "license": "MIT",
